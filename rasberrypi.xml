  <project path="RasberryPi/u-boot" name="raspherypi_u-boot.git" revision="master" />
  <project path="RasberryPi/kernel" name="raspherypi_kernel.git" revision="master" />
  <project path="RasberryPi/rootfs" name="raspherypi_rootfs.git" revision="master" />
  <project path="RasberryPi/toolchain" name="raspherypi_toolchain.git" revision="master" />

</manifest>

